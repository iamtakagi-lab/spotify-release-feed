  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "build": "parcel build --target node src/index.ts",
    "dev": "ts-node-dev --respawn src/index.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "lint:prettier": "prettier . --check",
    "lint:eslint": "eslint . --format friendly",
    "lint": "run-p lint:*",
    "fix:prettier": "yarn lint:prettier --write",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix": "run-s fix:eslint fix:prettier",
    "test": "jest",
    "test:watch": "jest --coverage --silent=false --verbose false --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@types/koa-router": "^7.4.2",
    "axios": "^0.21.1",
    "cron": "^1.8.2",
    "discord.js": "^12.5.3",
    "http-status": "^1.5.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-logger": "^3.2.1",
    "koa-requestid": "^2.1.0",
    "koa-router": "^10.0.0",
    "querystring": "^0.2.1",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "@types/cron": "^1.7.3",
    "@types/koa": "^2.13.3",
    "@types/koa-bodyparser": "^4.3.1",
    "@types/koa-helmet": "^6.0.3",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-requestid": "^2.0.2",
    "@types/koa__cors": "^3.0.2",
    "@types/node": "^16.3.2",
    "@types/spotify-web-api-node": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "autoprefixer": "10.2.3",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react": "1.1.7",
    "eslint-formatter-friendly": "7.0.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "^27.0.4",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.3.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
